<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/zhongyaoxing"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="需求工程的重要性"
        android:textSize="30dp"
        android:textColor="@android:color/background_dark"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/huoquwenti"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="需求获取的常见问题"
        android:textSize="30dp"
        android:layout_marginTop="16dp"
        android:textColor="@android:color/background_dark"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/zhuyaoneirong"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="需求工程的主要内容"
        android:textSize="30dp"
        android:layout_marginTop="16dp"
        android:textColor="@android:color/background_dark"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/qishirenwu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="需求工程的起始任务"
        android:textSize="30dp"
        android:layout_marginTop="16dp"
        android:textColor="@android:color/background_dark"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/daochuguocheng"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="需求工程的导出需求过程"
        android:textSize="30dp"
        android:layout_marginTop="16dp"
        android:textColor="@android:color/background_dark"
        android:textStyle="bold"/>


</LinearLayout>
